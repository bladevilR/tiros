<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.third.jdx.mapper.BuMaterialEntryOrderThirdMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.third.jdx.bean.BuMaterialEntryOrder">
        <id column="id" property="id"/>
        <result column="entry_no" property="entryNo"/>
        <result column="entry_date" property="entryDate"/>
        <result column="entry_dept_id" property="entryDeptId"/>
        <result column="company_id" property="companyId"/>
        <result column="workshop_id" property="workshopId"/>
        <result column="line_id" property="lineId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, entry_no, entry_date, entry_dept_id, company_id, workshop_id, line_id
    </sql>

    <select id="selectByEntryNo" resultType="org.jeecg.modules.third.jdx.bean.BuMaterialEntryOrder">
        select *
        from bu_material_entry_order
        where entry_no = #{entryNo}
    </select>

</mapper>
