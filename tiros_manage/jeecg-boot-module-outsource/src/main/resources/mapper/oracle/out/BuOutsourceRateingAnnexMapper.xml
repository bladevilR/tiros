<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.outsource.mapper.BuOutsourceRateingAnnexMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.outsource.bean.BuOutsourceRateingAnnex">
        <id column="id" property="id" />
        <result column="rate_id" property="rateId" />
        <result column="file_name" property="name" />
        <result column="upload_date" property="updateTime" />
        <result column="file_size" property="fileSize" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, rate_id, file_name, upload_date, file_size
    </sql>
    <delete id="deleteFileById">
        delete  from  bu_doc_file where id=#{id}
    </delete>

</mapper>
