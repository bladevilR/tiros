<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.basemanage.org.mapper.BuUserExtendMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.basemanage.org.bean.BuUserExtend">
        <id column="user_id" property="userId" />
        <result column="postion_id" property="postionId" />
        <result column="system_id" property="systemId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, postion_id, system_id
    </sql>

    <!-- 插入value sql -->
    <sql id="insertValueItems">
        <trim suffixOverrides=",">
            #{item.userId, jdbcType=VARCHAR},
            #{item.postionId, jdbcType=VARCHAR},
            #{item.systemId, jdbcType=VARCHAR}
        </trim>
    </sql>


    <insert id="insertList" parameterType="list">
        insert into bu_user_extend
        (
        <include refid="Base_Column_List"/>
        )
        <foreach collection="list" item="item" index="index" separator="union all">
            (
            select
            <include refid="insertValueItems"/>
            from dual
            )
        </foreach>
    </insert>

</mapper>
