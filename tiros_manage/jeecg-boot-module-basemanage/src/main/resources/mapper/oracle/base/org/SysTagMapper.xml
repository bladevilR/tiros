<mapper namespace="org.jeecg.modules.basemanage.org.mapper.SysTagMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.basemanage.org.bean.SysTag">
        <id column="id" property="id" />
