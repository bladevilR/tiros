<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.basemanage.regu.mapper.BuRepairReguPersonMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.basemanage.regu.bean.BuRepairReguPerson">
        <id column="id" property="id"/>
        <result column="regu_detail_id" property="reguDetailId"/>
        <result column="amount" property="amount"/>
        <result column="require_postion" property="requirePostion"/>
        <result column="require_certificate" property="requireCertificate"/>
        <result column="require_tech" property="requireTech"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, regu_detail_id, amount, require_postion, require_certificate, require_tech
    </sql>

    <!-- 插入value sql -->
    <sql id="insertValueItems">
        <trim suffixOverrides=",">
            #{item.id},
            #{item.reguDetailId},
            #{item.amount},
            #{item.requirePostion},
            #{item.requireCertificate},
            #{item.requireTech},
        </trim>
    </sql>


    <insert id="insertList" parameterType="java.util.List">
        INSERT INTO bu_repair_regu_person
        (
        <include refid="Base_Column_List"/>
        )
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (
            <include refid="insertValueItems"/>
            )
        </foreach>
    </insert>

</mapper>
