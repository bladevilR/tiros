<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.system.mapper.SysDictItemMapper">

    <select id="selectAllList" resultType="org.jeecg.modules.system.entity.SysDictItem">
        select t.item_text,
               t.item_value,
               t1.dict_code
        from sys_dict_item t
                 left join sys_dict t1 on t1.id = t.dict_id
    </select>

</mapper>
