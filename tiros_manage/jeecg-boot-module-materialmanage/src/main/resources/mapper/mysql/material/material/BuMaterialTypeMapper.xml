        <where>
            <if test="queryVO.type != null">
                and t.kind = #{queryVO.type}
            </if>
            <if test="queryVO.attr != null">
                and t.category2 = #{queryVO.attr}
            </if>
            <if test="queryVO.searchText != null and queryVO.searchText != ''">
                and (
