<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.material.entry.mapper.BuMaterialEntryLevelFourDetailMapper">



    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,enter_order_id,enter_detail_id,matertial_type_id,self_warehouse_id, amount,opration_user_id,opration_date
    </sql>

    <!-- 插入value sql -->
    <sql id="insertValueItems">
        <trim suffixOverrides=",">
            #{item.id, jdbcType=VARCHAR},
            #{item.entryOrderId, jdbcType=VARCHAR},
            #{item.enterDetailId, jdbcType=VARCHAR},
            #{item.materialTypeId, jdbcType=VARCHAR},
            #{item.selfWarehouseId, jdbcType=VARCHAR},
            #{item.amount, jdbcType=DOUBLE},
            #{item.oprationUserId, jdbcType=VARCHAR},
            #{item.oprationDate, jdbcType=DATE},
        </trim>
    </sql>

    <insert id="insertBatch" parameterType="java.util.List">
        <if test="list != null and list.size > 0">
            INSERT INTO bu_material_entry_4_detail
            (
            <include refid="Base_Column_List"/>
            )
            <foreach collection="list" item="item" index="index" separator="union all">
                (
                select
                <include refid="insertValueItems"/>
                from dual
                )
            </foreach>
        </if>

    </insert>


</mapper>
