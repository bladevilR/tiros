<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.dispatch.planform.mapper.BuWorkCheckTechLinkDispatchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.dispatch.planform.bean.BuWorkCheckTechLink">
        <id column="id" property="id"/>
        <result column="check_id" property="checkId"/>
        <result column="teck_book_id" property="teckBookId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, check_id, teck_book_id
    </sql>

    <select id="selectListByWorkCheckId" resultType="org.jeecg.modules.dispatch.planform.bean.BuWorkCheckTechLink">
        select t.*,
               t1.file_name as teckBookName
        from bu_work_check_tech_link t
                 left join bu_repair_tech_book t1 on t.teck_book_id = t1.id
        where t.check_id = #{workCheckId}
    </select>

</mapper>
